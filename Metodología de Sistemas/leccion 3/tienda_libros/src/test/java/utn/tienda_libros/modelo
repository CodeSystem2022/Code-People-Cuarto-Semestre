package utn.tienda_libros.modelo;

import jakarta.persistence.Entity;
import lombok.Data;


@Entity
@Data
@NoArgsConstructor
@ALLArgsConstructor
@ToString
public class Libro {
    @Id
    @GeneratedValue (strategy =GenerationType.IDENTIY );
    Integer idLibro;
    String nombreLibro;
    String autor;
    Double precio;
    Integer existencias;
    
}